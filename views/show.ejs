    <script>

        var slideCount = $('#scoringSlider ul.slider li.slider').length;
        var slideWidth = $('#scoringSlider ul.slider li.slider').width();
        var slideHeight = $('#scoringSlider ul.slider li.slider').height();
        var sliderUlWidth = slideCount * slideWidth ;

        console.log( 'slideCount' , slideCount , '\n', 'slideWidth', slideWidth, '\n', 'slideHeight', slideHeight, '\n', 'sliderUlWidth', sliderUlWidth);

        $('#scoringSlider').css({ width: slideWidth, height: slideHeight });
        
        $('#scoringSlider ul.slider').css({ width: sliderUlWidth});

        $('#scoringSlider ul.slider li.slider').css({ width: slideWidth - (slideWidth*.2)});
        
        // $('#scoringSlider ul.slider li.slider:last-child').prependTo('#scoringSlider ul.slider');

        function moveLeft() {
            $('#scoringSlider ul').animate({
                left: + slideWidth
            }, 200, function () {
                $('#scoringSlider ul li:last-child').prependTo('#scoringSlider ul');
                $('#scoringSlider ul').css('left', '');
            });
        };

        function moveRight() {
            $('#scoringSlider ul').animate({
                left: - slideWidth
            }, 200, function () {
                $('#scoringSlider ul li:first-child').appendTo('#scoringSlider ul');
                $('#scoringSlider ul').css('left', '');
            });
        };

        $('a.control_prev').click(function () {
            moveLeft();
    
        });

        $('a.control_next').click(function () {
            moveRight();

        });

    </script>



    
