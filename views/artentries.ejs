<%- include('partials/header')%>

<script>
   $(document).ready(function(){
          $("li.nav-item.dropdown.myCategories").attr("style", "background-color:orange;");
          $("span.nav-link.dropdown-toggle.myCategories").attr("style", "color:#fff;");
       });
</script>

<h1  class="fullSpan">
  <span class="darkOrange font-weight-bold"><%=  pageCategoryId.categoryId %>: <%= categorySpecifics[letterIndexKeys[0][pageCategoryId.categoryId]] .type  %> </span>
<br>
  <span  class="myCategorySpecific" ><%= categorySpecifics[letterIndexKeys[0][pageCategoryId.categoryId]].subtype  %></span>        
   <span class="white"><%=  categorySpecifics[letterIndexKeys[0][pageCategoryId.categoryId]].specific  %></span>       

   </h1>



<script>
    let url = new URLSearchParams(window.location.search);
  let pageCategoryId = url.get("categoryId");

  let getEl = document.querySelector("#setPageCategoryId");
  getEl.setAttribute("data", pageCategoryId);

  console.log(pageCategoryId);
</script>
<div id="setPageCategoryId" data=""></div>

<article class="appendixContainer">
