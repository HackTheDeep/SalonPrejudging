       if( artentries[i].category ===  pageCategoryId.categoryId ){   %>


      <div class="col-lg-4 col-md-6 col-sm-6 mt-5 <%= findScore.id %>" id="item<%= i %>" >
        <div class="img-thumbnail " >
      

         

        <a href="/artentries/<%= artentries[i]._id %>?<%= pageCategoryId.categoryId %>" >  
         <img src="<%= artentries[i].link %>" class="card-img-top" alt="Image for scoring" > </a>

  
